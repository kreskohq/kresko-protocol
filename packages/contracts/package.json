{
  "name": "@kreskolabs/contracts",
  "description": "Typechain and contract deployments for front-end",
  "version": "6.0.15",
  "type": "module",
  "author": {
    "name": "Kresko Labs",
    "url": "https://www.kresko.fi"
  },
  "keywords": [
    "blockchain",
    "ethereum",
    "hardhat",
    "smart-contracts",
    "solidity",
    "typechain"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "prepublish": "npm version patch"
  },
  "license": "ISC",
  "devDependencies": {
    "tsc-alias": "^1.8.2",
    "tsup": "^7.1.0",
    "typescript": "5.1.6"
  }
}
