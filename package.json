{
    "name": "@kreskolabs/protocol",
    "description": "Kresko solidity sources",
    "version": "1.0.0",
    "author": {
        "name": "Kresko Labs",
        "url": "https://kresko.fi"
    },
    "typings": "globals.d.ts",
    "devDependencies": {
